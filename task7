
#include <iostream>

int main()
{
    int number;
    std::cin >> number;

    int firstDigit = number / 1000;
    int secondDigit = (number / 100) % 10;
    int thirdDigit = (number / 10) % 10;
    int fourthDigit = number % 10;

    bool isFirstOdd = (firstDigit % 2 != 0);

    int maxDigit = firstDigit;

    if (secondDigit > maxDigit)
    {
        maxDigit = secondDigit;
    }

    if (thirdDigit > maxDigit)
    {
        maxDigit = thirdDigit;
    }

    if (fourthDigit > maxDigit)
    {
        maxDigit = fourthDigit;
    }

    if (isFirstOdd && firstDigit == maxDigit)
    {
        std::cout << "Is odd digit and its greather than others" << std::endl;
    }
    else
    {
        std::cout << "Its not odd and its not greather than others" << std::endl;
    }
}
