#include <iostream>

int main()
{
    int monthOfYear;
    std::cin >> monthOfYear;
    
    switch (monthOfYear)
    {
    case 1:
    case 2:
    case 12:
        std::cout << "The season is winter" << std::endl;
        break;
    case 3:
    case 4:
    case 5:
        std::cout << "The season is spring" << std::endl;
        break;
    case 6:
    case 7:
    case 8:
        std::cout << "The season is summer" << std::endl;
        break;
    case 9:
    case 10:
    case 11:
        std::cout << "The season is autumn" << std::endl;
        break;
    default:
        std::cout << "The season is invalid" << std::endl;
        break;
    }
