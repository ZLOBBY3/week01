#include <iostream>

int main()
{
    int year;
    std::cin >> year;
    bool isLeapYear = false;

    if (year % 4 == 0)
    {
        isLeapYear = true;
        if (year % 100 && year % 400)
        {
            isLeapYear = true;
            std::cout << year << " " << "is a leap year" << std::endl;
        }
    }
    else
    {
        isLeapYear = false;
        std::cout << year << " " << " is not leap year" << std::endl;
    }
}

