#include <iostream>

int main()
{
    int a,b;
    std::cin >> a >> b;
    
    if (a == 0)
    {
        if (b == 0)
        {
            std::cout << "INF" << std::endl;
        }
        else
        {
            std::cout << "no solution" << std::endl;
        }
    }
    else
    {
        int x = -b / a;
        std::cout << x << std::endl;
    }
}

